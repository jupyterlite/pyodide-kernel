+                }
+                catch {
+                    body = undefined;
+                }
+            }
+            return r.callback.call(null, {
+                pathname,
+                body,
+                query: Object.fromEntries(url.searchParams),
+            }, ...matches);
+        }
+        throw new Error('Cannot route ' + req.method + ' ' + req.url);
+    }
+    /**
+     * Add a new route.
+     *
+     * @param method The method
+     * @param pattern The pattern
+     * @param callback The callback
+     */
